        this.progediestPk = progediestPk;
    }

    public Date getProgediestFecha() {
        return progediestFecha;
    }

    public void setProgediestFecha(Date progediestFecha) {
        this.progediestFecha = progediestFecha;
    }

    public EstadosEditor getProgediestEstediFk() {
        return progediestEstediFk;
    }

    public void setProgediestEstediFk(EstadosEditor progediestEstediFk) {
        this.progediestEstediFk = progediestEstediFk;
    }

    public SsUsuario getProgediestUsuFk() {
        return progediestUsuFk;
    }

    public void setProgediestUsuFk(SsUsuario progediestUsuFk) {
        this.progediestUsuFk = progediestUsuFk;
    }

    public Programas getProgediestProgFk() {
        return progediestProgFk;
    }

    public void setProgediestProgFk(Programas progediestProgFk) {
        this.progediestProgFk = progediestProgFk;
    }

    @Override
    public int hashCode() {
        int hash = 0;
        hash += (progediestPk != null ? progediestPk.hashCode() : 0);
        return hash;
    }

    @Override
    public boolean equals(Object object) {
        // TODO: Warning - this method won't work in the case the id fields are not set
        if (!(object instanceof ProgEditorEstado)) {
            return false;
        }
        ProgEditorEstado other = (ProgEditorEstado) object;
        if ((this.progediestPk == null && other.progediestPk != null) || (this.progediestPk != null && !this.progediestPk.equals(other.progediestPk))) {
            return false;
        }
        return true;
    }

    @Override
    public String toString() {
        return "com.sofis.entities.data.ProgEditorEstado[ progediestPk=" + progediestPk + " ]";
    }

