<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<ui:composition xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns="http://www.w3.org/1999/xhtml"
                xmlns:ace="http://www.icefaces.org/icefaces/components"
                xmlns:c="http://java.sun.com/jsp/jstl/core"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:h="http://java.sun.com/jsf/html"
                xmlns:icecore="http://www.icefaces.org/icefaces/core"
                xmlns:ice="http://www.icesoft.com/icefaces/component">

    <script type="text/javascript">
        // slight update to account for browsers not supporting e.which
        function disableF5(e) {
            if ((e.which || e.keyCode) == 116)
                e.preventDefault();
        }
        ;
        // To disable f5
        // jQuery menor a 1.7
        $(document).bind("keydown", disableF5);
        /* OR jQuery >= 1.7 */
        $(document).on("keydown", disableF5);
    </script>

    <ice:form id="publicacionWebForm" 
              rendered="#{!inicioMB.usuarioAdmin}">
        <!-- Titulo -->
        <ice:panelGroup id="tituloPagina" styleClass="tituloPagina">
            <ice:outputLabel value="#{labels.pub_web_titulo}"/>
        </ice:panelGroup>

        <!-- FORMULARIO FILTRO BUSQUEDA -->
        <ui:include src="./../fragmentos/publicacionWebFiltro.xhtml" />

        <!-- Datos generales de la ficha -->
        <ice:panelGroup id="listadoProgProyEdicion">

            <ice:panelSeries var="itemArea" value="#{publicacionWebMB.filtroInicioResultado}">
                <ice:panelGroup styleClass="proyPendientes margen-sup">
                    <ice:panelGroup>
                        <ice:outputText value="#{itemArea.area.areaNombre}" />
                    </ice:panelGroup>

                    <ace:dataTable id="tblResultadosPorArea1" 
                                   value="#{itemArea.primerNivel}"  
                                   var="item" 
                                   styleClass="itemsEdicion"
                                   paginator="false" 
                                   pageCount="5" 
                                   rows="100">
                        <ace:column headerText="">
                            <ui:include src="./../fragmentos/publicacionWebFicha.xhtml" />
                        </ace:column>
                    </ace:dataTable>

                </ice:panelGroup>
            </ice:panelSeries>
        </ice:panelGroup>
    </ice:form>
</ui:composition>