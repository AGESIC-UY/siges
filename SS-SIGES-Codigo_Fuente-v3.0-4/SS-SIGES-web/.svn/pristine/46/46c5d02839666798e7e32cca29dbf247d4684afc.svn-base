package com.sofis.web.delegates;

import com.sofis.business.ejbs.EditorEstadoBean;
import com.sofis.entities.data.ProgEditorEstado;
import com.sofis.entities.data.ProyEditorEstado;
import java.util.List;
import javax.inject.Inject;

/**
 *
 * @author Usuario
 */
public class EditorEstadoDelegate {

    @Inject
    private EditorEstadoBean editorEstadoBean;

    public List<ProgEditorEstado> obtenerProgHistorico(Integer fichaFk) {
        return editorEstadoBean.obtenerProgHistorico(fichaFk);
    }

    public List<ProyEditorEstado> obtenerProyHistorico(Integer fichaFk) {
        return editorEstadoBean.obtenerProyHistorico(fichaFk);
    }
}
