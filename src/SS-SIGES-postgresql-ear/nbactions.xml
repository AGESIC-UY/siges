<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>CUSTOM-generar-diff</actionName>
        <displayName>liquibase-generar-diff</displayName>
        <goals>
            <goal>resources:resources</goal>
            <goal>liquibase:diff</goal>
        </goals>
        <properties>
            <liquibase.diffChangeLogFile>${project.build.directory}/db.changelog_siges_lastest.xml</liquibase.diffChangeLogFile>
            <liquibase.changeLogFile>${project.build.directory}/db.changelog_siges_lastest.xml</liquibase.changeLogFile>
            <liquibase.migrationSqlOutputFile>${project.build.directory}/db.changelog_siges_lastest.sql</liquibase.migrationSqlOutputFile>
            <liquibase.diffIncludeCatalog>false</liquibase.diffIncludeCatalog>
            <liquibase.diffIncludeSchema>false</liquibase.diffIncludeSchema>
            <liquibase.url>${siges.imm.liquibase.url}</liquibase.url>
            <liquibase.driver>${siges.imm.liquibase.driver}</liquibase.driver>
            <liquibase.username>${siges.imm.liquibase.user}</liquibase.username>
            <liquibase.password>${siges.imm.liquibase.pass}</liquibase.password>
            <liquibase.referenceUrl>${siges.imm.liquibase.reference-url}</liquibase.referenceUrl>
            <liquibase.referenceDriver>${siges.imm.liquibase.reference-driver}</liquibase.referenceDriver>
            <liquibase.referenceUsername>${siges.imm.liquibase.reference-user}</liquibase.referenceUsername>
            <liquibase.referencePassword>${siges.imm.liquibase.reference-pass}</liquibase.referencePassword>
            <liquibase.verbose>${siges.imm.liquibase.verbose}</liquibase.verbose>
            <liquibase.logging>${siges.imm.liquibase.logging}</liquibase.logging>
            <liquibase.outputFileEncoding>UTF-8</liquibase.outputFileEncoding>
            <liquibase.referenceDefaultSchemaName>siges</liquibase.referenceDefaultSchemaName>
            <liquibase.defaultSchemaName>siges</liquibase.defaultSchemaName>
        </properties>
    </action>
    <action>
        <actionName>CUSTOM-generar-sql</actionName>
        <displayName>liquibase-generar-sql</displayName>
        <goals>
            <goal>resources:resources</goal>
            <goal>liquibase:updateSQL</goal>
        </goals>
        <properties>
            <liquibase.diffChangeLogFile>${project.build.directory}/db.changelog_siges_lastest.xml</liquibase.diffChangeLogFile>
            <liquibase.changeLogFile>${project.build.directory}/db.changelog_siges_lastest.xml</liquibase.changeLogFile>
            <liquibase.migrationSqlOutputFile>${project.build.directory}/db.changelog_siges_lastest.sql</liquibase.migrationSqlOutputFile>
            <liquibase.diffIncludeCatalog>false</liquibase.diffIncludeCatalog>
            <liquibase.diffIncludeSchema>false</liquibase.diffIncludeSchema>
            <liquibase.url>${siges.imm.liquibase.url}</liquibase.url>
            <liquibase.driver>${siges.imm.liquibase.driver}</liquibase.driver>
            <liquibase.username>${siges.imm.liquibase.user}</liquibase.username>
            <liquibase.password>${siges.imm.liquibase.pass}</liquibase.password>
            <liquibase.referenceUrl>${siges.imm.liquibase.reference-url}</liquibase.referenceUrl>
            <liquibase.referenceDriver>${siges.imm.liquibase.reference-driver}</liquibase.referenceDriver>
            <liquibase.referenceUsername>${siges.imm.liquibase.reference-user}</liquibase.referenceUsername>
            <liquibase.referencePassword>${siges.imm.liquibase.reference-pass}</liquibase.referencePassword>
            <liquibase.verbose>${siges.imm.liquibase.verbose}</liquibase.verbose>
            <liquibase.logging>${siges.imm.liquibase.logging}</liquibase.logging>
            <liquibase.outputFileEncoding>UTF-8</liquibase.outputFileEncoding>
        </properties> 
    </action>
    <action>
        <actionName>CUSTOM-liquibase-update</actionName>
        <displayName>liquibase-update</displayName>
        <goals>
            <goal>resources:resources</goal>
            <goal>liquibase:update</goal>
        </goals>
        <properties>
            <liquibase.diffChangeLogFile>${project.build.directory}/db.changelog_siges_lastest.xml</liquibase.diffChangeLogFile>
            <liquibase.changeLogFile>${project.build.directory}/db.changelog_siges_lastest.xml</liquibase.changeLogFile>
            <liquibase.migrationSqlOutputFile>${project.build.directory}/db.changelog_siges_lastest.sql</liquibase.migrationSqlOutputFile>
            <liquibase.diffIncludeCatalog>false</liquibase.diffIncludeCatalog>
            <liquibase.diffIncludeSchema>false</liquibase.diffIncludeSchema>
            <liquibase.url>${siges.imm.liquibase.url}</liquibase.url>
            <liquibase.driver>${siges.imm.liquibase.driver}</liquibase.driver>
            <liquibase.username>${siges.imm.liquibase.user}</liquibase.username>
            <liquibase.password>${siges.imm.liquibase.pass}</liquibase.password>
            <liquibase.referenceUrl>${siges.imm.liquibase.reference-url}</liquibase.referenceUrl>
            <liquibase.referenceDriver>${siges.imm.liquibase.reference-driver}</liquibase.referenceDriver>
            <liquibase.referenceUsername>${siges.imm.liquibase.reference-user}</liquibase.referenceUsername>
            <liquibase.referencePassword>${siges.imm.liquibase.reference-pass}</liquibase.referencePassword>
            <liquibase.verbose>${siges.imm.liquibase.verbose}</liquibase.verbose>
            <liquibase.logging>${siges.imm.liquibase.logging}</liquibase.logging>
            <liquibase.outputFileEncoding>UTF-8</liquibase.outputFileEncoding>
        </properties>
    </action>
    <action>
        <actionName>CUSTOM-fingbugs-report</actionName>
        <displayName>fingbugs-report</displayName>
        <goals>
            <goal>site</goal>
        </goals>
        <properties>
            <xmlOutput>true</xmlOutput>
            <xmlOutputDirectory>${project.build.directory}</xmlOutputDirectory>
            <onlyAnalyze>com.sofis.*</onlyAnalyze>
        </properties>
    </action>
    <action>
        <actionName>CUSTOM-generate-depgraph</actionName>
        <displayName>generate-depgraph</displayName>
        <goals>
            <goal>com.github.ferstl:depgraph-maven-plugin:2.1.0:graph</goal>
        </goals>
        <properties>
            <createImage>true</createImage>
            <graphFormat>dot</graphFormat>
            <showVersions>true</showVersions>
            <showDuplicates>true</showDuplicates>
            <showGroupIds>true</showGroupIds>
            <showDuplicates>true</showDuplicates>
            <showConflicts>true</showConflicts>
        </properties>
    </action>
</actions>